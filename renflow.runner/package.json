{
  "name": "renflow.runner",
  "version": "1.0.0",
  "description": "OneBot-RenFlow runner",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./nodes": {
      "import": "./dist/nodes/index.js",
      "types": "./dist/nodes/index.d.ts"
    },
    "./workflow": {
      "import": "./dist/workflow/index.js",
      "types": "./dist/workflow/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "run:json": "tsx src/index.ts",
    "run:json:prod": "node dist/index.js",
    "prepare": "npm run build"
  },
  "bin": {
    "renflow-runner": "bin/renflow-runner"
  },
  "keywords": [
    "onebot",
    "workflow",
    "automation",
    "qq",
    "bot"
  ],
  "author": "Stapx Steve",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/codemirror": "^5.60.17",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^24.9.1",
    "@types/shell-quote": "^1.7.5",
    "codemirror": "^5",
    "codemirror-editor-vue3": "^2.8.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "amqplib": "^0.10.3",
    "class-transformer": "^0.5.1",
    "fastq": "^1.11.0",
    "jsonpath": "^1.1.1",
    "mri": "^1.2.0",
    "reflect-metadata": "^0.2.2",
    "shell-quote": "^1.8.3",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "@types/adm-zip": "^0.5.7"
  },
  "optionalDependencies": {
    "puppeteer": "^24.28.0"
  }
}
